{
    "name": "Canvas-Drive Sync",
    "description" : "Synchronizes UC Davis Canvas LMS files to Google Drive",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "assets/material/sync/ic_sync_black_24dp_2x.png"
    },
    "key": "",
    "oauth2": {
        "client_id": "386193826857-rarrpv572ur6ho4ha334k3v6kehjss8q.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive"
        ]
    },
    "permissions": [
        "identity",
        "tabs",
        "https://canvas.ucdavis.edu/",
        "https://www.googleapis.com/",
        "https://*.instructure.com/",
        "https://*.amazonaws.com/"
    ]
}
