{
    "name": "Canvas-Google Drive Sync",
    "description" : "Synchronizes UC Davis Canvas LMS files to Google Drive",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "ic_sync_black_24dp_2x.png"
    },
    "permissions": [
        "https://canvas.ucdavis.edu/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
